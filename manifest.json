{
    "name": "Twitch Bonus Clicker",
    "description": "Simple extension, which automatically collects Twitch Bonus received for watching streams.",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
      "128": "icon_128.png",
      "48": "icon_48.png",
      "16": "icon_16.png"
    },
    "content_scripts": [
        {
            "matches": ["*://*.twitch.tv/*"],
            "js": ["contentScript.js"]
        }
    ]
  }